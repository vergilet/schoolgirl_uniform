class <%= class_name.camelcase %>Controller < SchoolgirlUniform::BaseController

  def initialize_form
    @form = <%= class_name.camelcase %>Form.new(session[session_key] || {})
  end

  def finish; end

  def form_attributes
    [:username, :password, :email, :phone]
  end

  def paths
    {
      next: next_<%= class_name.snakecase %>_path,
      current: current_<%= class_name.snakecase %>_path,
      previous: previous_<%= class_name.snakecase %>_path,
      create: <%= class_name.snakecase %>_path
    }
  end
end
